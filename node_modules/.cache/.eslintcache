js",[],[],[],"/Users/soorena/projects/AZMAA/azmma/src/App.js",["95"],[],"import \"./App.css\";\nimport { BrowserRouter as Router, Routes, Route } from \"react-router-dom\";\nimport Navbar from \"./components/navbar\";\nimport Envoy from \"./components/envoy\";\nimport Vote from \"./components/vote\";\nimport Actions from \"./components/actions\";\nimport Blog from \"./components/blog\";\nimport AboutMe from \"./components/about-me\";\nimport Home from \"./components/home\";\nimport LogIn from \"./components/log-in\";\nimport Footer from\"./components/footer\";\nfunction App() {\n  return (\n    <div className=\"App\">\n      <Navbar />\n      <Routes>\n        <Route exact path=\"/\" element={<Home />} />\n        <Route path=\"/envoy\" element={<Envoy />} />\n        <Route path=\"/votes\" element={<Vote />} />\n        <Route path=\"/actions\" element={<Actions />} />\n        <Route path=\"/blog\" element={<Blog />} />\n        <Route path=\"/about-me\" element={<AboutMe />} />\n        <Route path=\"/log-in\" element={<LogIn />} />\n      </Routes>\n      <Footer/>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/soorena/projects/AZMAA/azmma/src/reportWebVitals.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/navbar/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/envoy/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/actions/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/vote/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/blog/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/about-me/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/home/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/log-in/index.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/hook/useWidth.js",[],[],"/Users/soorena/projects/AZMAA/azmma/src/components/footer/index.js",["96","97"],[],"import React from \"react\";\nimport styled from \"styled-components\";\nimport data from \"../../data.json\";\nimport azmma from \"../../assets/azmaa.svg\";\n\nconst FooterContainer = styled.section`\n  background-color: #095644;\n  padding: 19px 20px;\n  @media (min-width: 480px) {\n    padding: 30px 10%;\n    display: flex;\n    justify-content: space-between;\n  }\n`;\n\nconst AboutUs = styled.div`\n  display: none;\n  @media (min-width: 480px) {\n    display: flex;\n    width: 65%;\n    flex-direction: column;\n    h4 {\n      color: #dff5f0;\n      font-size: 1.87vw;\n      font-weight: bold;\n      margin: 0;\n    }\n    p {\n      color: #dff5f0;\n      font-size: 1.25vw;\n      font-weight: light;\n      line-height: 2.344vw;\n      text-align: justify;\n      overflow: hidden;\n      text-overflow: ellipsis;\n      display: -webkit-box;\n      -webkit-line-clamp: 4;\n      line-clamp: 4;\n      -webkit-box-orient: vertical;\n      margin-top:5px;\n      margin-bottom:0;\n    }\n  }\n`;\n\nconst ContactUs = styled.div`\n  & > .item {\n    display: flex;\n    margin-bottom: 10px;\n    gap: 17px;\n\n    span {\n      width: 45px;\n      height: 40px;\n      img {\n        width: 100%;\n        height: 100%;\n        object-fit: contain;\n      }\n    }\n\n    .content {\n      width: 85%;\n      background-color: #f3f3f3;\n      border-radius: 4px;\n      display: flex;\n      a {\n        margin: auto 10px;\n        color: #095644;\n        text-decoration: none;\n        width: 100%;\n        text-align: left;\n        font-size: 5.85vw;\n        font-weight: light;\n        direction: ltr;\n      }\n    }\n  }\n\n  @media (min-width: 480px) {\n    width: 30%;\n    & > .item {\n      margin-bottom: 20px;\n      span {\n        width: 76px;\n        height: 67px;\n      }\n      .content {\n        width: 80%;\n        a {\n          font-size: 1.87vw;\n        }\n      }\n    }\n  }\n`;\n\nconst Copyright = styled.div`\n  min-height: 42px;\n  background-color: #ffffff;\n  display: flex;\n  p {\n    margin: auto;\n    color: #cbcbcb;\n    white-space: nowrap;\n    display: flex;\n    div {\n      background-image: url(${azmma});\n      background-size: contain;\n      padding-right: 5px;\n      padding-left: 5px;\n      opacity: 0.5;\n      background-repeat: no-repeat;\n      width: 29px;\n      height: 24px;\n    }\n  }\n`;\n\nexport default function Footer() {\n  const contactItem = data.footer.map((x, i) => {\n    return (\n      <div className=\"item\">\n        <span>\n          <img src={x.icon} />\n        </span>\n        <div className=\"content\">\n          <a target=\"_blank\" href={x.href}>\n            {x.contact}\n          </a>\n        </div>\n      </div>\n    );\n  });\n\n  return (\n    <div>\n      <FooterContainer>\n        <ContactUs>{contactItem}</ContactUs>\n        <AboutUs>\n          <h4>کمی در بارۀ ازما:</h4>\n          <p>\n            لورم ایپسوم متن ساختگی با تولید سادگی نامفهوم از صنعت چاپ، و با\n            استفاده از طراحان گرافیک است، چاپگرها و متون بلکه روزنامه و مجله در\n            ستون و سطرآنچنان که لازم است، و برای شرایط فعلی تکنولوژی مورد نیاز،\n            و کاربردهای متنوع با هدف بهبود ابزارهای کاربردی می باشد، کتابهای\n            زیادی در شصت و سه درصد گذشته حال و آینده، شناخت فراوان جامعه و\n            متخصصان را می طلبد، تا با نرم افزارها شناخت بیشتری را برای طراحان\n            رایانه ای علی الخصوص طراحان خلاقی، و فرهنگ پیشرو در زبان فارسی ایجاد\n            کرد، در این صورت می توان امید داشت که تمام و دشواری موجود در ارائه\n            راهکارها، و شرایط سخت تایپ به پایان رسد و زمان مورد نیاز شامل\n            حروفچینی دستاوردهای اصلی، و جوابگوی سوالات پیوسته اهل دنیای موجود\n            طراحی اساسا مورد استفاده قرار گیرد.\n          </p>\n        </AboutUs>\n      </FooterContainer>\n      <Copyright>\n        <p>\n          همۀ حقوق نزد <div></div> محفوظ است.\n        </p>\n      </Copyright>\n    </div>\n  );\n}\n","/Users/soorena/projects/AZMAA/azmma/src/components/home/components/slider.js",[],[],"/Users/soorena/pro